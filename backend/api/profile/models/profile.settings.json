{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "profiles",
  "info": {
    "name": "profile"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "firstName": {
      "type": "string",
      "minLength": 2,
      "maxLength": 30,
      "required": false
    },
    "lastName": {
      "type": "string",
      "required": false,
      "minLength": 2,
      "maxLength": 30
    },
    "birthday": {
      "type": "date",
      "required": false
    },
    "bio": {
      "type": "richtext",
      "maxLength": 150,
      "minLength": 1
    },
    "exercises": {
      "collection": "exercise"
    }
  }
}
